<core:FragmentDefinition xmlns="sap.ui.comp.valuehelpdialog" xmlns:core="sap.ui.core" xmlns:fb="sap.ui.comp.filterbar" xmlns:m="sap.m">
	<ValueHelpDialog
		title="{VHKeyModel>/Title}"
		ok="onVHOK"
		cancel="onVHCancel"
		afterClose="onVHAfterClose"
		supportMultiselect="true"
		key="{VHKeyModel>/key}"
		descriptionKey="{VHKeyModel>/descriptionKey}">
		<filterBar>
			<fb:FilterBar showGoOnFB="false" showFilterConfiguration="false"/>
		</filterBar>
	</ValueHelpDialog>
</core:FragmentDefinition>